        "dev": "cross-env NODE_ENV=development TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts",
        "demo": "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./src/server.ts"
